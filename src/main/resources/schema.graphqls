schema {
    query: Query
}

type Query {
    track(id: ID!) : Track
}

type Track {
    id : ID!
    title : String!
    artworkUrl: String!
}

type Like {
    id : ID!
    type : String!
    typeId: String!
}

type Mutation {
    createTrack(input: CreateTrackInput!): Track
    createLike(input: CreateLikeInput!): Like
}

input CreateTrackInput {
    title: String!
    artworkUrl: String!
}

input CreateLikeInput {
    type: String!
    typeId: String!
}
